@model List<MvcDemo.Models.Student>
    @{
        ViewBag.Title = "Show All";
    }

<h2>List All Detail</h2>
<hr/>
<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>#</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var std in Model)
            {
                <tr>
                    <td>@std.RollNo</td>
                    <td>@std.FName </td>
                    <td>@std.LName</td>
                    <td><a href="Default/Edit">Edit</a></td>
                </tr>
            }
        </tbody>
        <tfoot>
            <tr>
                <td colspan="3">
                    @Html.ActionLink("Back To lIst", "Index")
                </td>
            </tr>
        </tfoot>
    </table>
</div>

